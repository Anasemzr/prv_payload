func (t *Tunnel) handleSocks(src io.ReadWriteCloser) error {
    // Connect to your SOCKS5 proxy server
    proxyConn, err := net.Dial("tcp", "proxy.example.com:1080") // Replace with your proxy server address
    if err != nil {
        return err
    }
    defer proxyConn.Close()

    // Forward data between source and proxy
    go func() {
        _, _ = io.Copy(proxyConn, src)
    }()
    _, err = io.Copy(src, proxyConn)
    if err != nil {
        return err
    }

    return nil
}
